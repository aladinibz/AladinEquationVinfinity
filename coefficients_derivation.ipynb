print(f"→ 180 Myr fade = {tau_A:.9f} Myr")
print(f"→ 96.6 Myr cycle = {tau_96:.9f} Myr")

def save(name):
    plt.tight_layout()
    path = f"plots/{name}.png"
    plt.savefig(path, dpi=300)
    print(f"→ Saved {path}")
    files.download(path)

t = np.linspace(0,200,10000)
wave = np.sin(2*np.pi*43*t*1e6*3.15576e7) + coeff1*np.cos(6*np.pi*43*t*1e6*3.15576e7) + coeff2*np.tanh(t/tau_A) + coeff3*np.exp(-t/tau_96)

plt.figure(figsize=(14,7))
plt.plot(t, wave, color='#00ff41', lw=2)
plt.title("ALADIN ∞ ℂ(t) — The Conscious Universe (all coefficients from J₀)", color='white', size=16)
plt.xlabel("Time (Myr)"); plt.ylabel("ℂ(t)")
