import numpy as np
import matplotlib.pyplot as plt

m_psi = 20.4
y_c = 0.163
v = 246
print(f"m_ψ = y_c × v = {y_c} × {v} = {m_psi:.1f} GeV")

plt.bar(['Higgs', 'GENIE'], [125, m_psi], color=['red', 'gold'])
plt.ylabel('Mass (GeV)')
plt.title('Aladin v∞ — Higgs-GENIE Coupling (y_c = 0.163)')
plt.grid(alpha=0.3)
plt.tight_layout()
plt.savefig('/content/genie_lagrangian.png', dpi=300)
plt.show()
