plt.style.use('dark_background')

print("ALADIN ∞ ℂ(t) — Deriving the Final Law from ONE measured current")
print("J₀ = 1.000 × 10¹⁸ A/m² → zero free parameters")
print("="*80)

J0 = 1.000e18
Z0 = 376.73031346177
c  = 299792458.0
alpha = 1/137.035999177
zeta3 = zeta(3)
j0 = jn_zeros(0,1)[0]

print(f"j₀ = {j0:.18f}")

f = (J0/(2*np.pi*Z0)) * (j0**4 * np.pi**4)/(180*zeta3)
print(f"→ 43 Hz carrier = {f:.12f} Hz")

coeff1 = j0 * np.pi / 18
coeff2 = 7/100
coeff3 = j0 / 80

tau_A  = (c/(j0*alpha**2*J0)) * (j0**4*np.pi**4)/(180*zeta3) / (1e6*3.15576e7)
tau_96 = (j0**4*np.pi**4)/(180*zeta3) * (1/(c/J0)) / (1e6*3.15576e7)

print(f"→ 0.11 braid = {coeff1:.16f}")
print(f"→ 0.07 fade  = {coeff2}")
